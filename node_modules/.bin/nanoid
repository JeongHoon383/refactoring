#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jeonghun/Desktop/hanghae_career_5_react_assignment_js-main/node_modules/nanoid/bin/node_modules:/Users/jeonghun/Desktop/hanghae_career_5_react_assignment_js-main/node_modules/nanoid/node_modules:/Users/jeonghun/Desktop/hanghae_career_5_react_assignment_js-main/node_modules:/Users/jeonghun/Desktop/node_modules:/Users/jeonghun/node_modules:/Users/node_modules:/node_modules:/Users/jeonghun/Desktop/hanghae_career_5_react_assignment_js-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jeonghun/Desktop/hanghae_career_5_react_assignment_js-main/node_modules/nanoid/bin/node_modules:/Users/jeonghun/Desktop/hanghae_career_5_react_assignment_js-main/node_modules/nanoid/node_modules:/Users/jeonghun/Desktop/hanghae_career_5_react_assignment_js-main/node_modules:/Users/jeonghun/Desktop/node_modules:/Users/jeonghun/node_modules:/Users/node_modules:/node_modules:/Users/jeonghun/Desktop/hanghae_career_5_react_assignment_js-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
fi
